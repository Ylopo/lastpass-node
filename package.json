{
  "name": "lastpass",
  "version": "1.0.3",
  "description": "A Lastpass client for Node.js!",
  "main": "./dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "example": "rollup -c ./rollup.config.example.js && node ./dist-example/index.js",
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "lint": "eslint -c ./package.json ./src ./example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfrankland/lastpass-node.git"
  },
  "keywords": [
    "lastpass",
    "passwords",
    "client"
  ],
  "author": "Dylan Frankland <dylan@frankland.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfrankland/lastpass-node/issues"
  },
  "homepage": "https://github.com/dfrankland/lastpass-node#readme",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "form-data": "^2.3.2",
    "html-to-text": "^4.0.0",
    "node-fetch": "^2.1.1",
    "sanitize-filename": "^1.6.0",
    "source-map-support": "^0.5.4",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb"
  }
}
